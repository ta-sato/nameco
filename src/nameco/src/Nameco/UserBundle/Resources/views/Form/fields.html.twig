{% block field_errors %}
{% spaceless %}
    {% if errors|length > 0 %}
    <ul class="formerrorlist unstyled text-error">
        {% for error in errors %}
            <li>{{ error.messageTemplate|trans(error.messageParameters, 'validators') }}</li>
        {% endfor %}
    </ul>
    {% endif %}
{% endspaceless %}
{% endblock field_errors %}

